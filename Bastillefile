CMD mkdir -p /usr/local/etc/pkg/repos
CMD echo 'FreeBSD: { url: 'pkg+http://pkg.FreeBSD.org/\$\{ABI\}/latest', enabled: yes }' > /usr/local/etc/pkg/repos/FreeBSD.conf
PKG neovim git mc tmux curl wget zsh sudo python311 exa lazygit moar
CMD git clone https://github.com/olejak/nvim.git /root/.config/nvim
